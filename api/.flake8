[flake8]
ignore = C101, D403, F403, F405, I100, I201, W503, W504, E126, F841
exclude =
    .git,
    __pycache__,
    .mypy_cache,
    venv,
    */migrations/*,
    */tests.py*,
    *settings/*,
    */utils.py,
filename = *.py
accept-encodings = utf-8
inline-quotes = single
max-complexity = 12
max-linenumber = 500
max-line-length = 119
multiline-quotes = double
