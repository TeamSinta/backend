{
  "name": "frontend",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/styled": "^11.11.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@mui/material": "^5.13.2",
    "@mui/styled-engine-sc": "^5.12.0",
    "@react-oauth/google": "^0.9.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.0.4",
    "@testing-library/user-event": "^12.0.9",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "axios": "^1.3.4",
    "msw": "^1.2.2",
    "msw-storybook-addon": "^1.8.0",
    "node-globals": "^0.1.5",
    "react": "^17.0.2",
    "react-cookie": "^4.1.1",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.12.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "sass": "^1.62.1",
    "styled-components": "^5.3.9",
    "vite-plugin-svgr": "^3.2.0",
    "web-vitals": "^2.1.4",
    "react-player": "^2.12.0",
    "@videosdk.live/react-sdk": "^0.1.75"
  },
  "scripts": {
    "start": "VITE_BACKEND_URL=http://localhost:3000 VITE_ENV=develop vite",
    "mock": "VITE_BACKEND_URL=http://localhost:3001 VITE_ENV=mock vite",
    "build": "vite build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "VITE_BACKEND_URL=http://localhost:6006 VITE_ENV=story storybook dev -p 6006 -s public",
    "build-storybook": "storybook build -s public",
    "prepare": "cd .. && HUSKY_GIT_DIR=.git npx husky install frontend/.husky",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.0.18",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-interactions": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/addon-styling": "^1.0.8",
    "@storybook/node-logger": "^7.0.18",
    "@storybook/preset-create-react-app": "^7.0.18",
    "@storybook/react": "^7.0.18",
    "@storybook/react-vite": "^7.0.18",
    "@storybook/testing-library": "^0.1.0",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.0",
    "prettier": "^2.8.7",
    "prop-types": "^15.8.1",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "storybook": "^7.0.18",
    "storybook-addon-react-router-v6": "^1.0.2",
    "typescript": "^4.9.4",
    "vite": "^4.3.9",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-node-polyfills": "^0.9.0",
    "vite-tsconfig-paths": "^4.2.0",
    "webpack": "^5.76.3"
  },
  "eslintIgnore": [
    "/.eslintrc.js"
  ]
}
